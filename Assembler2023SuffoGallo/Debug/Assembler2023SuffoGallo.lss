                                 	  CALL ESPERAR_TX
                                 	  LDI R20, ' '
                                 	  CALL ESPERAR_TX
                                 	  LDI R20, ' '
                                 	  RET
                                 
                                 	  UART_MOSTRAR_ALARMA:
