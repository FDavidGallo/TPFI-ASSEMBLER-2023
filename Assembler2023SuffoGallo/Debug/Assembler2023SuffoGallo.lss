                                 ; Para incluir esto: >.include "LecturaAdc.inc"<, para llamarlo: >CALL InicioADC <
                                 ; Esto esta diseado para leer una de las entradas analogicas
                                 ; Este algoritmo lo que hace es  ir cargando en las posiciones de Z (tres seguidas)
                                 ;  los valores  del adc (Conversor analogico digital) del Atmega328p.
                                 ;   Atencin, antes de entrar en/llamar a  esto, debes tener cargado:
                                 ;			>Z en la posicin de memoria flash inicial (si tens miedo de sobreescritura entre datos us separadores)
                                 ;			>El sensor usado (de 0 a 6) en R5
                                 ;           >El ADC configurado de la siguiente manera:
                                 ;																ldi r16,0b01100000
                                 ;																sts ADMUX, r16
                                 ;																ldi r16,0b10000111
                                 ;																sts ADCSRA, r16
                                 ; Es preciso tambien que los sensores estn conectados, como mximo, a 5V, y mnimo a 0 V.									
                                 ;	Por el resto de los registros no te preocupes, ya que esta diseado para que se reestauren al  terminar...
                                 ; Prest atencin: Si bien estamos usando "la mayor presicin posible",
                                 ; debe tenerse en cuenta que no se usarn los nmeros de 0000 a 1024 SINO QUE
                                 ;  se usa un sistema para leer, como mximo valor 255,7 (5) (el 5 se ha redondeado a 0) o, cambiando la coma  de lugar, 2557;
                                 ; si te sirve usalo.
                                 ;  Al terminar la ejecucin tendremos: Z='centena', Z+1='decena', Z+2='unidad', Z=R18 (la coma); guardados todos
                                 ;					   en formato ASCII TEN EN CUENTA ESO--> Numeros Normales (/ARABIGOS/Decimales)= ASCII - 48
                                 ; by: Gallito, aguante el software libre (?)
                                 
                                 .def unidad = r20
                                 .def decena = r21
                                 .def centena = r22
                                 
                                 InicioADC:
                                 
                                 ;			jmp GuardarCosasAdc ;Guardamos lo que haca el micro
                                 							 ;Setemos todo a 0:
                                 								 InicioADC2:
00151e e040                      			ldi unidad,0
00151f e050                      			ldi decena, 0
001520 e060                      			ldi centena,0
001521 e020                      			ldi R18,0
001522 e000                      			ldi r16, 0b00000000 ;Opcional, limpiar el puerto D
001523 b907                      			out DDRC, r16
001524 c000                      			rjmp CICLO_ADC ;Este es nuestro ciclo de conversin
                                 
                                 CICLO_ADC:
001525 940e 155a                 	call LeerADC
001527 940e 1561                 	call EsperarADC
001529 9120 0078                 	lds r18,ADCL ;Cargamos en R18 la parte Baja del ADC, contiene los dos bits menos significativos o nuestra "COMA"
00152b 9130 0079                 	lds r19,ADCH ;Cargamos en R18 la parte alta del ADC , contiene los 8 bits ms significativo, va de 000 a 255
00152d e040                      	ldi unidad,0
00152e e050                      	ldi decena,0
00152f e060                      	ldi centena,0
001530 2f03                      	mov r16,r19; Metemos a R16 ADCL
001531 940e 1538                 	call ASCII		; Llamamos a la funcin ASCII 
001533 e310                      	ldi r17,48		; le sumamos la diferencia entre el ASCII Y DECIMAL, PARA QUE SE GUARDE EN ASCII
001534 0f41                      	add unidad, r17
001535 0f51                      	add decena, r17
001536 0f61                      	add centena,r17
                                 	; Ahora, teniendo todo en ASCII, pasamos a guardarlas una por una en la memoria:
                                 
001537 9508                      	ret; Que largue todo y volvamos a lo que estaba haciendo el micro
                                 
                                 ASCII: ;Vamos a obtener unidad, decena, centena y "Coma":
                                 		L_CENTENA:
001538 3604                      				cpi r16,100  ; El numero es menor a 100?
001539 f018                      				BRLO L_DECENA; Vaya a "L_Decena"
00153a 9563                      				inc centena  ; Si no es as, incremente en uno las centenas
00153b 5604                      				subi r16,100 ; Restele 100 al nmero
00153c cffb                      				rjmp L_CENTENA ;Repita, hasta que el numero sea menor a 100
                                 				;Si todo fue realizarse correctamente, solamente podr ejecutarse todo dos veces
                                 				; Recuerde: en 8 bits, el nmero ms grande a almacenar es el 256, contando el 0, 255.
                                 		L_DECENA: ;Hace exactamente lo mismo que el anterior, solo que 100 en lugar de 10
