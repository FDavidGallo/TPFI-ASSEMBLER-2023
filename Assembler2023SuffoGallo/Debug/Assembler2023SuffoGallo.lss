                                 ASCII: ;Vamos a obtener unidad, decena, centena y Coma:
                                 		L_CENTENA:
001209 3604                      				cpi r16,100  ; El numero es menor a 100?
00120a f018                      				BRLO L_DECENA; Vaya a "L_Decena"
00120b 9563                      				inc centena  ; Si no es as, incremente en uno las centenas
00120c 5604                      				subi r16,100 ; Restele 100 al nmero
00120d cffb                      				rjmp L_CENTENA ;Repita, hasta que el numero sea menor a 100
                                 				;Si todo fue realizarse correctamente, solamente podr ejecutarse todo dos veces
                                 				; recuerde, en 8 bits, el nmero ms grande a almacenar es el 256, contando el 0, 255.
                                 		L_DECENA: ;Hace exactamente lo mismo que el anterior, solo que 100 en lugar de 10
00120e 300a                      				cpi r16,10 
00120f f018                      				BRLO L_UNIDAD
001210 9553                      				inc decena
001211 500a                      				subi r16,10
001212 cffb                      				rjmp L_DECENA
                                 		L_UNIDAD: ;Idem con 1
001213 3001                      				cpi r16,1
001214 f018                      				BRLO L_FIN ; Esta indica que se termin de leer la parte baja del ADC
001215 9543                      				inc unidad
001216 5001                      				subi r16,1
001217 cffb                      				rjmp L_UNIDAD
                                 		L_FIN:
