                                 	 CALL ESPERAR_TX
                                 	 RET
                                 
                                 
                                 	;Configuracion Inicio SDIV
                                 	  UART_MOSTRAR_INICIO:
                                 	  LDI R20, 'R'
                                 	  CALL ESPERAR_TX	  	
